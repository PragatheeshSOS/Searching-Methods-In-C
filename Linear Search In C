// LINEAR SEARCH...

#include<stdio.h>

int main()
{
    int n,i,t,c=0;
    printf("Enter Limit: ");
    scanf("%d",&n);
    printf("Enter Elements: ");
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter Target Element: ");
    scanf("%d",&t);
    
    for(i=0;i<n;i++)
    {
        //c+=1;
        if(a[i]==t)
        {
            c=1;
            printf("Element Position: ");
            printf("%d",i+1);
            break;
        }
    }
    
    if(c==0)    //if(c==n)
    {
        printf("Target Doesn't Exits\t");
        printf("-1");
    }
}

//Code By Pragatheesh.S (githubcom/PragatheeshSOS)

/*

INPUT 1:
5
10 20 30 40 50
30

OUTPUT 1:
Element Position: 3

INPUT 2:
4
10 30 40 50
20

OUTPUT 2:
Target Doesn't Exits    -1

*/

