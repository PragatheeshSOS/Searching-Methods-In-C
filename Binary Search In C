//BINARY SEARCH...

#include<stdio.h>

int main()
{
    int n,m,t,c=0;
    printf("Enter Limit: ");
    scanf("%d",&n);
    
    int a[n];
    printf("Enter Elements: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    printf("Enter Target Element: ");
    scanf("%d",&t);
    
    int s=0,e=n-1; //e=n;
    while(s<e) //while(s<=e)
    {
        m = s+e/2;
        
        if(a[m]==t)
        {
            c=1;
            printf("Element Found At %d",m+1);
            break;
        }
        
        if(a[m]<t)
        {
            s=m; //s=m+1;
        }
        
        if(a[m]>t)
        {
            e=m; //e=m-1;
        }
    }
    
    if(c==0)
    {
        printf("Target Doesn't Exits\t -1");
    }
    
}

